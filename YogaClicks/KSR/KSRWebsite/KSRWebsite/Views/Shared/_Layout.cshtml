<body>

    @Html.Partial("Header")

    <div class=" container body-content nopadding">


        @RenderBody()

    </div>
<footer>
    <div class="row nopadding footermain nomargin">
        <div class="col-sm-9 nopadding">
            <p>
                KSR Resources <i class="fa fa-copyright" aria-hidden="true"></i> @DateTime.Now.Year - Registered company in England and Wales<br/>
                Company Number: 7510085<br/>
                VAT Number: 128 9846 65
            </p>

        </div>
        <div class="col-sm-3 nopadding footerSocial">

            <a target="_blank" href="https://www.linkedin.com/company/ksr-resources"><i class="fa fa-linkedin" aria-hidden="true"></i></a>
            <a target="_blank" href="https://twitter.com/ksrresources"><i class="fa fa-twitter" aria-hidden="true"></i></a>
            <a target="_blank" href="https://www.facebook.com/ksrresources16/"><i class="fa fa-facebook" aria-hidden="true"></i></a>


            


        </div>
    </div>


</footer>
    @Styles.Render("~/bundles/css")


    <link href="http://allfont.net/allfont.css?fonts=franklin-gothic-medium" rel="stylesheet" type="text/css" />
    <link href='https://fonts.googleapis.com/css?family=Crimson+Text:400italic' rel='stylesheet' type='text/css'>
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
