@model dynamic

@{
    ViewBag.Title = "THE YOGA MAP PROJECT";
    Layout = "~/Views/Layout/layout.cshtml";
}
@section LeftNav{
    @Html.Partial("left-nav-static-footer")
}
@section Head {
    <meta property="og:title" content="Let us help you transform life’s challenges and create the life you want" />
    <meta property="og:description" content="We’ll inspire you with real life stories, inform you with proper clinical studies, and equip you with the creations of people on the same journey - clothes, malas, gifts, books..." />
    <meta property="og:image" content="http://www.yogaclicks.com/images/global/facebookshare-Generic.png" />
}



<div style="margin-left: -15px;margin-right:-15px" class="nopadding hidden-sm hidden-md hidden-lg">
    <section class="filters greyScale" id="filter-results">
        <div id="filter-wrap">
            <div class="the-sub-menu addpadding">
                <a href="#" id="right-menu" class="pull-right" title="Filter menu"><span class="glyphicon glyphicon-filter filter-icon"></span></a>
                <h1>THE YOGA MAP PROJECT</h1>
            </div>
        </div>
    </section>
</div>



<div class="row nopadding">
    <div class="col-xs-12 ">

        <section class="page-content QLinks" style="min-height: 1100px;">
            <article id="static">
                <h1 class="hidden-xs">THE YOGA MAP PROJECT</h1><br />
                <div class="row no-padding">
                    <div class="col-xs-12 nopadding">
                        <img class="img-responsive" src="~/Images/static/Poweredbyyoga/tymp-logo.png" /><br /><br />
                        <a href="@Url.Action("index","yogamap")">  How does yoga make a difference to you? Share your story and inspire someone today. Together we can get the world on its mat! »</a><br /><br />
                        <img class="img-responsive" src="~/Images/static/Poweredbyyoga/1-what.png" /><br />
                        <img class="img-responsive" src="~/Images/static/Poweredbyyoga/2-how.png" /><br />
                        <img class="img-responsive" src="~/Images/static/Poweredbyyoga/3-involved.png" /><br />
                        <img class="img-responsive" src="~/Images/static/Poweredbyyoga/4-tell.png" /><br />
                        <a href="@Url.Action("index","yogamap")"> How does yoga make a difference to you? Share your story and inspire someone today. Together we can get the world on its mat! »</a><br /><br /><br /><br />

                    </div>


                </div>

            </article>




        </section>
</div>
</div>

