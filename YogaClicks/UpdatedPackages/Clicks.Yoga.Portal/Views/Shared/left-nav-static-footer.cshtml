<div id="navwrap">
    
    <section class="filters" id="filter-results">
        <div id="filter-wrap">
            <div class="the-sub-menu">
                <nav class="filter text-justify centerAlign ">

                    <span id="right-menu-close"  title="Close" class="close filter-icon pull-right hidden-sm hidden-md hidden-lg" aria-label="Close"><span aria-hidden="true">&times;</span></span>

                    <h3>Quick links</h3>
                    <div class="list-group">
                        <a class="list-group-item" href="@Url.Action("Lucys-story", "static")">Lucys Story</a>
                        <a class=" hidden" href="@Url.Action("PoweredByYoga", "static")">#PoweredByYoga</a>
                        <a class="list-group-item" href="@Url.Action("yogamap", "static")">The Yoga Map Project</a>

                        <a class="list-group-item" href="@Url.Action("sell-with-us", "static")">Sell With Us</a>


                        <a class=" hidden" href="@Url.Action("YCAmbassador", "static")">Become a YC Ambassador</a>
                        @*<a class="list-group-item" href="@Url.Action("Faq", "static")">FAQ's</a>*@





                        <a class="list-group-item" href="@Url.Action("site-map", "static")">Site Map</a>
                        <a class="list-group-item" href="@Url.Action("privacy-policy", "static")">Privacy</a>
                        <a class="list-group-item" href="@Url.Action("terms-of-use", "static")">Terms of Use</a>
             

                    </div>
                    <h3>Contact Us</h3>

                        <div class="col-xs-12 nopadding">


                            <div class="form-group" id="GOemail">
                                @Html.TextBox("EUName", "", new { @class = "form-control TextGline", placeholder = "Name", style = "margin-bottom:6px!important;" })
                                @Html.TextBox("EUEmail", "", new { @class = "form-control TextGline", placeholder = "Email address", style = "margin-bottom:6px!important;" })
                                @Html.TextArea("EUMessage", "", new { @class = "form-control TextGline", placeholder = "Your message", style = "margin-bottom:6px!important;" })
                                <span id="message"></span>
                            </div>
                           
                            <span id="GoemailB" class="btn btn-success pull-right ">Get in touch!</span>

                            <br />
                            <br />
<br />
                        </div>


                    <h3>Find us</h3>
                    <div class="col-xs-12 nopadding">
                        <div class="center-block finduson">
                            
                            <a href="https://www.instagram.com/yogaclicks/" target="_blank"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                            <a href="https://twitter.com/yogaclicks" target="_blank"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                            <a href="https://www.facebook.com/yogaclicks/" target="_blank"><i class="fa fa-facebook" aria-hidden="true"></i></a>
                        </div>




                    </div>
                </nav>
            </div>
        </div>
    </section>
</div>
<script type="text/javascript">
    $('#GoemailB').click(function () {
    
 
            $.ajax({
                type: 'POST',
                url: '@Url.Action("UsEmail","static")',
                async: true,
                data: {
                    email: $('#EUName').val(), name: $('#EUEmail').val(), message: $('#EUMessage').val()
        },
                success: function (html) {
                    if (html === "success") {
                        $('#message').html("<br/><span class='alert alert-success '>Your message has been sent.</span><br/><br/>");
                    } else {
                        $('#message').html("<br/><span class='alert alert-danger '>Oh snap! Please fill in the above.</span><br/><br/>");
                    }
               


            }
        });
    });

    $("#right-menu-close").click(function () {

        $("#right-nav").fadeToggle("fast");
        $(".dark-overlay").fadeToggle("slow");

    });

</script>
