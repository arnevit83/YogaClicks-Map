@model dynamic

@{
    ViewBag.Title = "Yoga Practice, Yoga Styles, Yoga Poses";
    ViewBag.Description = "Need help developing your yoga practice? We’ll help you find yoga styles and yoga poses, whether you’re a yoga beginner or an advanced human pretzel.";
    ViewBag.MenuClass = "how-to-navigation";
    Layout = "~/Views/Layout/Layout.cshtml";
   // ViewBag.MenuSection = "Practice";
}
@section LeftNav{

    @Html.Partial("left-nav-static-footer-ad")

}
@section Head {
    <meta property="og:title" content="Let us help you transform life’s challenges and create the life you want" />
    <meta property="og:description" content="We’ll inspire you with real life stories, inform you with proper clinical studies, and equip you with the creations of people on the same journey - clothes, malas, gifts, books..." />
    <meta property="og:image" content="http://www.yogaclicks.com/images/global/facebookshare-Generic.png" />
}





<div id="content-slider" class="content-slider">
    <div class="practice-strip "></div>
    <div class="single-content landingPage practicePage" id="scroll">

        <section class="page-content">

            <article id="static">
                    <header>
                        <h2 class="pageTitle"><img src="~/images/SvgIcons/icon_practice.svg" data-svgpng="/images/SvgIcons/icon_practice.png" /><span class="colorTitle">Styles &amp; Poses</span><span class="stylesMobiletitle"> - Discover your yoga style, strike a pose</span></h2>
                    </header>
                <div class="imageWrap">
                    <div class="image">
                        <img src="/images/practice-landing.jpg" class="img-responsive"/>
                    </div>
                    <div class="caption">
                        Image courtesy of Robert Sturman
                    </div>
                </div>



                <div class="navWrap">
                    <ul><li>
    <a href="@Url.Action("Quiz", "Static")">
